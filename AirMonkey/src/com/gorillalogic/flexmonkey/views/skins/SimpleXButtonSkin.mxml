<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
             xmlns:s="library://ns.adobe.com/flex/spark"
             width="20"
             height="20"
             alpha.disabled="0.5">

    <fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

    <!-- states -->
    <s:states>
        <s:State name="up"/>
        <s:State name="over"
                 stateGroups="overDown"/>
        <s:State name="down"
                 stateGroups="overDown"/>
        <s:State name="disabled"/>
    </s:states>

    <s:Rect left="0"
            right="0"
            bottom="0"
            top="0">
        <s:fill>
            <s:SolidColor color="#000000"
                          alpha="0"/>
        </s:fill>
    </s:Rect>

    <!-- x -->
    <s:Path left="5"
            top="5">
        <s:data>M 0,0 L 2,0 5,3 8,0 10,0 10,2 7,5 10,8 10,10 8,10 5,7 2,10 0,10 0,8 3,5 0,2 Z</s:data>
        <s:fill>
            <s:SolidColor id="iconColor"
                          color="#cccccc"
                          color.overDown="#000000"/>
        </s:fill>
    </s:Path>
</s:SparkSkin>
