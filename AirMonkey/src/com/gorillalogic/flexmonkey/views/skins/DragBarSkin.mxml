<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:mx="library://ns.adobe.com/flex/mx">

    <!-- host component -->
    <fx:Metadata>
		[HostComponent("spark.components.supportClasses.SkinnableComponent")]
	</fx:Metadata>

    <!-- states -->
    <s:states>
        <s:State name="up"/>
        <s:State name="overDown"/>
    </s:states>

    <s:Rect left="0"
            right="0"
            bottom="0"
            top="0">
        <s:fill>
            <s:SolidColor color="#000000"
                          alpha="0"/>
        </s:fill>
    </s:Rect>

    <s:Line width="10"
            top="0"
            horizontalCenter="0">
        <s:stroke>
            <s:SolidColorStroke color="#999999"
                                color.overDown="#000000"
                                weight="2"/>
        </s:stroke>
    </s:Line>

    <s:Line width="10"
            top="4"
            horizontalCenter="0">
        <s:stroke>
            <s:SolidColorStroke color="#999999"
                                color.overDown="#000000"
                                weight="2"/>
        </s:stroke>
    </s:Line>

    <s:Line width="10"
            top="8"
            horizontalCenter="0">
        <s:stroke>
            <s:SolidColorStroke color="#999999"
                                color.overDown="#000000"
                                weight="2"/>
        </s:stroke>
    </s:Line>

</s:Skin>
