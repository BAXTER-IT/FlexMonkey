<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
             xmlns:s="library://ns.adobe.com/flex/spark"
             width="20"
             height="20"
             alpha.disabled="0.5">

    <fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

    <!-- states -->
    <s:states>
        <s:State name="up"/>
        <s:State name="over"
                 stateGroups="overDown"/>
        <s:State name="down"
                 stateGroups="overDown"/>
        <s:State name="disabled"/>
    </s:states>

    <s:Rect top="0"
            bottom="0"
            left="0"
            right="0"
            alpha="0">
        <s:fill>
            <s:SolidColor color="#000000"/>
        </s:fill>
    </s:Rect>

    <s:Line  xFrom="0" yFrom="5" xTo="11" yTo="5">
        <s:stroke>
            <s:SolidColorStroke color="#999999"
                                color.overDown="#000000"
                                weight="1.5"/>
        </s:stroke>
    </s:Line>
	
	<s:Line  xFrom="0" yFrom="9" xTo="11" yTo="9">
		<s:stroke>
			<s:SolidColorStroke color="#999999"
								color.overDown="#000000"
								weight="1.5"/>
		</s:stroke>
	</s:Line>
	<s:Line  xFrom="0" yFrom="13" xTo="11" yTo="13">
		<s:stroke>
			<s:SolidColorStroke color="#999999"
								color.overDown="#000000"
								weight="1.5"/>
		</s:stroke>
	</s:Line>
	<s:Line  xFrom="0" yFrom="17" xTo="11" yTo="17">
		<s:stroke>
			<s:SolidColorStroke color="#999999"
								color.overDown="#000000"
								weight="1.5"/>
		</s:stroke>
	</s:Line>


</s:SparkSkin>
